{
  "$schema": https://vega.github.io/schema/vega-lite/v4.json
  title: Ethereum
  data: {
    url: {
      %context%: true
      %timefield%: date
      index: etherium
      body: {
        stored_fields: ["*"]
        script_fields: {}
        _source: {
          excludes: []
        }
        size: 150
      }
    }
    format: {property: "hits.hits"}
  }
  encoding: {
    x: {
      field: _source.date
      type: temporal
    }
    y: {
      field: _source.open
      type: quantitative
      scale: {zero: false},
      axis: {title: "Statistics"}
    },
    color: {
      condition: {
        test: "datum._source.open < datum._source.close",
        value: "#00BFB3"
      },
      value: "#990000"
    }
  },
  layer: [
    {
      mark: {type:"rule", stroke: "#708090"}
      encoding: {
        y: {field: "_source.low"},
        y2: {field: "_source.high"}
      }
    },
    {
      mark: {
        type: "bar"
      }
      encoding: {
        y: {field: "_source.open"},
        y2: {field: "_source.close"}
      }
    }
  ]
}